package br.com.alura;

public class Aula {

	private String titulo;
	private int    tempo;
	
	public Aula(String titulo, int tempo) {
		this.titulo = titulo;
		this.tempo  = tempo;
	}
	
	@Override
	public String toString() {
		return "Aula: " + this.titulo + "\nTempo: " + this.tempo + " minutos";
	}
}
