package br.com.alura;

import java.util.ArrayList;

public class TestandoListas {

	public static void main(String[] args) {
		ArrayList<String> cursos = new ArrayList<>();
		
		cursos.add("JAVA I");
		cursos.add("JAVA II");
		cursos.add("JAVA III");
		
		System.out.println("O 3º curso é: " + cursos.get(2));
	}
}
