package br.com.alura;

import java.util.Set;

public class TestaCursoComAluno {

	public static void main(String[] args) {

		Curso javaColecoes = new Curso("Dominando as coleções do JAVA", "Paulo Silveira");
		
		javaColecoes.matricula(new Aluno("Rodrigo Turine", 34672));
		javaColecoes.matricula(new Aluno("Guilherme Silveira", 34672));
		javaColecoes.matricula(new Aluno("Mauricio Aniche", 17645));
		
		Set<Aluno> alunos = javaColecoes.getAlunos();
		
		javaColecoes.matricula(new Aluno("Jaquim Coves", 98453));
		
		alunos.forEach(aluno -> {
			System.out.println(aluno);
		});
	}
}
